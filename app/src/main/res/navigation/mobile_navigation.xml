<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">
    <!-- app:startDestination="@+id/navigation_dashboard para que empiece en la tienda" -->

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.pawswhiskers.ui.informacion.InformacionFragment"
        tools:layout="@layout/fragment_tienda" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.pawswhiskers.ui.tienda.TiendaFragment"
        tools:layout="@layout/fragment_user" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.pawswhiskers.ui.usuario.UsuarioFragment"
        tools:layout="@layout/fragment_informacion" />

</navigation>